<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="acta_ir_sequence" model="ir.sequence">
        <field name="name">Secuencia actas</field>
        <field name="implementation">no_gap</field>
        <field name="code">secuencia_actas</field>
        <field name="prefix">AC</field>
        <field name="suffix">%(year)s</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
    </record>

     <record id="presupuesto_acta_ir_sequence" model="ir.sequence">
        <field name="name">Secuencia actas</field>
        <field name="implementation">no_gap</field>
        <field name="code">secuencia_presupuesto</field>
        <field name="prefix">P</field>
        <field name="suffix">%(year)s</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
    </record>
</odoo>
